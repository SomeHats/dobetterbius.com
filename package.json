{
  "name": "dobetterbius",
  "version": "1.0.0",
  "description": "The #DoBetterBiUs Manifesto ",
  "main": "index.js",
  "scripts": {
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html --out-dir dist --public-url https://dobetterbius.org/ --experimental-scope-hoisting"
  },
  "author": "Alex Dytrych <alex@dytry.ch>",
  "license": "ISC",
  "dependencies": {
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "parcel-bundler": "^1.9.7"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  }
}
